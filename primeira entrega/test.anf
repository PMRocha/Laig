		<appearanceref id="nonAppearance" />

		<primitives>
		</primitives>

		<descendants>
			<noderef id="pe1" />
			<noderef id="pe2" />
		</descendants>
	</node>

	<node id="pe1">
		<transforms>
			<transform type="translate" to="0.0 0.0 1.766"/>
		</transforms>

		<appearanceref id="inherit" />

		<primitives>
			<cylinder base="1.1" top="1.1" height="7.0" slices="6" stacks="5"/>
		</primitives>

		<descendants>
		</descendants>
