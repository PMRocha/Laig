	<node id="ring">
		<transforms>
			<transform type="translate" to="0.0 0.5 0.0"/>
		</transforms>

		<appearanceref id="inherit" />

		<primitives>
			<torus inner="0.5" outer="0.7" stacks="6" loops="6"/>
		</primitives>

		<descendants>
			<noderef id="blades"/>
		</descendants>
	</node>

	<node id="blades">
		<transforms>
			<transform type="rotate" axis="x" angle="90.0"/>
		</transforms>

		<appearanceref id="bladesAppearance" />

		<primitives>
			<rectangle xy1="-0.25 0.25" xy2="0.25 4.25"/>
			<rectangle xy1="0.25 0.25" xy2="4.25 -0.25"/>
			<rectangle xy1="0.25 -0.25" xy2="-0.25 -4.25"/>
			<rectangle xy1="-0.25 -0.25" xy2="-4.25 0.25"/>
